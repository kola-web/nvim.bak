      "export default {",
      "  components: {},",
      "  props: {},",
      "  data() {",
      "    return {",
      "      show: false",
      "    };",
      "  },",
      "  methods: {",
      "    getDetail() {},",
      "    showModal() {",
      "      this.getDetail();",
      "      this.show = true;",
      "    },",
      "    handleCancel(e) {",
      "      this.show = false;",
      "    }",
      "  }",
      "};",
      "</script>",
      "",
      "<style lang=\"scss\" scoped></style>",
      ""
    ]
  },
