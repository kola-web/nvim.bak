{
  "vbase": {
    "prefix": "vtab // @description",
    "description": "",
    "body": [
      "<template>",
      "  <div>",
      "    <fmbreadcrumb></fmbreadcrumb>",
      "    <a-row type=\"flex\" justify=\"space-between\" align=\"middle\" class=\"top-box\">",
      "      <a-col>",
